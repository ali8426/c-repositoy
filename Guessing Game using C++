#include <iostream>

using namespace std;

int main()
{
    int x= 127;
    int y;
    std::cout << "Guess what the number is (between 0-300 inclusive):" << std::endl;
    cin>>y;
    while(y != x){
        std::cout << "Try Again:" << std::endl;
        cin>>y;
    }
    
 std::cout << "Nice job you guessed my number!" << std::endl;
    return 0;
}
